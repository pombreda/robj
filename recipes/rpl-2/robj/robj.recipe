loadInstalled('python')
class Robj(PackageRecipe):
    name = 'robj'
    version = '0'

    buildRequires = ['make:runtime', 'procps:runtime', 'util-linux:runtime',
                     'lxml:python', 'xobj:python',]

    def setup(r):
        r.addMercurialSnapshot()
        r.MakeInstall()


